define(q, {string, string});
define(p, {string, string});
define(r, {string, string});

r(A, C) :-
  p(A, B),
  q(B, C);

p("foo", "bar");
q("bar", "baz");
q("bar", "bam");

\dump r

define(later, {string, string});

later(A, B) :- p(A, B);
later(A, B) :- q(A, B);

\dump later

define(j1, {int8});
define(j2, {int8});

j2(A + 1) :- j2(A), j1(B), A >= B, A <= 7;

j2(1);
\dump j2
j1(5);
\dump j2
j1(1);
\dump j2
