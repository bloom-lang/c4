define(q, keys(0), {string, string});
define(p, keys(0), {string, string});
define(r, keys(0), {string, string});

r(A, C) :-
  p(A, B),
  q(B, C);

p("foo", "bar");
q("bar", "baz");
q("bar", "bam");

\dump r

define(later, keys(0), {string, string});

later(A, B) :- p(A, B);
later(A, B) :- q(A, B);

\dump later
